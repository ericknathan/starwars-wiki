{
  "expo": {
    "name": "StarWarsWiki",
    "slug": "StarWarsWiki",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "starwars-wiki",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#161616"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.ericknathan.starwarswiki",
      "supportsTablet": true
    },
    "android": {
      "package": "com.ericknathan.starwarswiki",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#161616"
      }
    },
    "description": "Aplicativo sobre a franquia de filmes StarWars contendo listagem de filmes e personagens.",
    "githubUrl": "https://github.com/ericknathan/starwars-wiki"
  }
}
